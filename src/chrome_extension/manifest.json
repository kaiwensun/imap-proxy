{
    "name": "Emails",
    "description": "Email overview with local IMAP proxy",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "alarms",
        "storage"
    ],
    "action": {
        "default_popup": "popup/index.html",
        "default_icon": {
            "16": "/images/envelop/16x16.png",
            "32": "/images/envelop/32x32.png",
            "64": "/images/envelop/64x64.png",
            "128": "/images/envelop/128x128.png"
        }
    },
    "icons": {
        "16": "/images/envelop/16x16.png",
        "32": "/images/envelop/32x32.png",
        "64": "/images/envelop/64x64.png",
        "128": "/images/envelop/128x128.png"
    },
    "host_permissions": [
        "http://localhost:8888/"
    ]
}